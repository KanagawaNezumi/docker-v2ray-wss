{
    "inbounds": [
        {
            "port": 1080,
            "listen": "127.0.0.1",
            "protocol": "socks",
            "sniffing": {
                "enabled": true,
                "destOverride": ["http", "tls"]
            },
            "settings": {
                "auth": "noauth",
                "udp": false
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "$domain",
                        "port": 443,
                        "users": [
                            {
                                "id": "$uid",
                                "encryption":"none"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "security": "tls",
                "wsSettings": {
                    "path": "/$access"
                }
            },
            "mux": {
                "enabled": true, 
                "concurrency": 128
            }
        },

        {
            "protocol": "freedom",
            "settings": {},
            "tag": "direct"
        }
    ],

    "routing": {
        "domainStrategy": "IPOnDemand",
        "rules":[
            {
                "type": "field",
                "outboundTag": "direct",
                "domain":["geosite:cn"]
            },
            {
                "type": "field",
                "outboundTag": "direct",
                "ip": [
                    "geoip:cn",
                    "geoip:private"
                ]
            }
        ]
    }
}
